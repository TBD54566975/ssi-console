server {
    listen       80;
    charset utf-8;
    root    /usr/share/nginx/html;
    index   index.html index.htm;
    location /v1 {
      proxy_redirect off;
      proxy_pass http://${SSI_SERVICE}:8080/v1;
    }
    location / {
        root /usr/share/nginx/html;
        try_files $uri /index.html;
    }
}
